<Application x:Class="DimitrovaM_FinalProject2023.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DimitrovaM_FinalProject2023"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="SF Bold" TargetType="Button">
            <Setter Property="FontFamily" Value="/Fonts/#SF-PRO-DISPLAY-BOLD"/>
        </Style>
        <Style x:Key="SF Bold Label" TargetType="Label">
            <Setter Property="FontFamily" Value="/Fonts/#SF-PRO-DISPLAY-BOLD"/>
        </Style>
        <Style x:Key="SF Light Label" TargetType="Label">
            <Setter Property="Content" Value="/Fonts/#SF-PRO-DISPLAY-LIGHT"/>
        </Style>
        
    </Application.Resources>

</Application>
    
